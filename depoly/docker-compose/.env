# 设置时区
TZ=Asia/Shanghai
# 设置网络模式
NETWORKS_DRIVER=bridge

# PATHS ##########################################
# 宿主机上代码存放的目录路径
CODE_PATH_HOST=../../code
# 宿主机上Mysql Reids数据存放的目录路径
DATA_PATH_HOST=./data

# MYSQL ##########################################
# Mysql 服务映射宿主机端口号，可在宿主机127.0.0.1:3306访问
MYSQL_PORT=3306
MYSQL_USERNAME=admin
MYSQL_PASSWORD=123456
MYSQL_ROOT_PASSWORD=123456

# Mysql 可视化管理用户名称，同 MYSQL_USERNAME
MYSQL_MANAGE_USERNAME=admin
# Mysql 可视化管理用户密码，同 MYSQL_PASSWORD
MYSQL_MANAGE_PASSWORD=123456
# Mysql 可视化管理ROOT用户密码，同 MYSQL_ROOT_PASSWORD
MYSQL_MANAGE_ROOT_PASSWORD=123456
# Mysql 服务地址
MYSQL_MANAGE_CONNECT_HOST=mysql
# Mysql 服务端口号
MYSQL_MANAGE_CONNECT_PORT=3306
# Mysql 可视化管理映射宿主机端口号，可在宿主机127.0.0.1:1000访问
MYSQL_MANAGE_PORT=1000

# NACOS ##########################################
# NACOS 服务映射宿主机端口号，可在宿主机127.0.0.1:8848/nacos访问
NACOS_PORT=8848

# CONSUL ##########################################
# CONSUL 服务映射宿主机端口号，可在宿主机127.0.0.1:8500/访问
# CONSUL提供dns功能，可以让我们通过dig命令行来进行测试，CONSUL默认的dns端口是8600
# 命令行：dig @192.168.226.130 -p 8600 consul.service SRV
CONSUL_PORT=8500
CONSUL_DNS_PORT=8600

# YAPI ##########################################
# YAPI 服务映射宿主机端口号，可在宿主机127.0.0.1:3000/访问
# 利用账号yapi@qq.com，密码123456来登录
YAPI_PORT=3000
YAPI_USERNAME=yapi@qq.com
YAPI_PASSWORD=123456